{
	"manifest_version": 2,
	"name": "Latinx To Latino",
	"version": "1.0",
	"description": "Replaces the text 'Latinx' with 'Latino'.",
	"content_scripts": 
	[
		{
			"matches": ["*://*/*"],
			"js": ["content_script.js"],
			"run_at": "document_end"
		}
	],
	"icons": { 
		"16": "correction-tape_16x16.png",
		"32": "correction-tape_32x32.png",
		"48": "correction-tape_48x48.png",
	 "128": "correction-tape_128x128.png" }
}